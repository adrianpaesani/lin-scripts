alias act-ad='cd ~/utiles-odoo/adhoc/aeroo_docs && sudo git pull && cd'
alias act-ar='cd ~/utiles-odoo/adhoc/aeroo_reports && sudo git pull && cd'
alias act-oa='cd ~/utiles-odoo/adhoc/odoo-addons && sudo git pull && cd'
alias act-ar='cd ~/utiles-odoo/adhoc/odoo-argentina && sudo git pull && cd'
alias act-ol='cd ~/utiles-odoo/adhoc/odoo-logistic && sudo git pull && cd'
alias act-ow='cd ~/utiles-odoo/adhoc/odoo-web && sudo git pull && cd'
alias act-odoo='cd /opt/odoo/odoo-server && sudo git pull && cd'
alias act-sis='sudo apt-get update && sudo apt-get dist-upgrade'
alias odoo-log='less /var/log/odoo/odoo-server.log'
alias act-full='act-ad && act-ar && act-oa && act-ar && act-ol && act-ow && act-odoo && act-sis'
